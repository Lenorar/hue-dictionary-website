

<% include ../partials/boilerplate %>


<% word.forEach(words => { %>
  <div class="word">
    <h4>
        <%= words.title %>
    </h4>
    <h4>
        <%= words.definition %>
    </h4>
  </div>

  <form action="<%= `/words/${words.id}?_method=GET` %>">
    <input type="submit" value="see more" />
  </form>

<% }) %>



<% include ../partials/end %>


